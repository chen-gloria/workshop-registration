{% if is_granted('ROLE_ADMIN') or is_granted('ROLE_INSTRUCTOR') %}
    {% block navbar_toolkit %}
        <ul class="list-unstyled topbar-menu float-end mb-0">
            {% block translations %}{{ include('navbar/_toolkit/_translations.html.twig')}}{% endblock %}
            {% block profile %}{{ include('navbar/_toolkit/_profile.html.twig')}}{% endblock %}
        </ul>
    {% endblock %}
{% else %}

{# {% if not is_granted('ROLE_ADMIN') and not is_granted('ROLE_INSTRUCTOR')  %} #}
    {% block navbar_login_register %}
        <ul class="list-unstyled topbar-menu float-end mb-0">
            {{ include('navbar/_navbar_login_register.html.twig')}}
        </ul>
    {% endblock %}
{% endif %}
